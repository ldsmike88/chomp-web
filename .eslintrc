{
  "parser": "@babel/eslint-parser",
  "extends": [
    "eslint:recommended",
    "airbnb",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
  ],
  "rules": {
    "react/jsx-filename-extension": [0],
    "react/function-component-definition": [2, { "namedComponents": "arrow-function" }]
  },
  "env": {
    "jest": true,
    "browser": true
  }
}